{
   "version": 2,
   "builds": [
     {
       "src": "next.config.js",
       "use": "@vercel/next"
     },
     {
       "src": "package.json",
       "use": "@vercel/static-build"
     },
     {
       "src": "pages/**/*",
       "use": "@vercel/static"
     }
   ],
   "routes":[{ "src": "/(.*)", "dest": "/" }]
 }
